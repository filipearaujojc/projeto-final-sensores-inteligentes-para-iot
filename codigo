from imageai.Detection import VideoObjectDetection
import matplotlib as plt
import tensorflow as tf
import numpy as np
import scipy
import keras
import h5py

detector = VideoObjectDetection()

detector.setModelTypeAsYOLOv3()

from google.colab import drive
